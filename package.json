{
  "name": "jane.github.io",
  "description": "Jane Open Source",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Zac Anger",
    "url": "https://zacanger.com"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "postbuild": "rm -rf opensource && mv build opensource",
    "prebuild": "sortpack",
    "start": "react-scripts start"
  },
  "homepage": "https://jane.github.io/opensource",
  "dependencies": {
    "cxs": "6.2.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "3.0.0"
  },
  "devDependencies": {
    "@types/node": "11.13.8",
    "@types/react": "16.8.15",
    "@types/react-dom": "16.8.4",
    "sortpack": "1.1.5",
    "typescript": "3.4.5"
  },
  "browserslist": [
    ">0.5%",
    "ie >= 11"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
