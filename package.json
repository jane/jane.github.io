{
  "name": "jane.github.io",
  "description": "Jane Open Source",
  "version": "0.0.1",
  "author": {
    "name": "Zac Anger",
    "url": "https://zacanger.com"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "postbuild": "mv build opensource",
    "start": "react-scripts start"
  },
  "homepage": "https://jane.github.io",
  "dependencies": {
    "cxs": "6.2.0",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-scripts": "2.0.5"
  },
  "devDependencies": {
    "husky": "1.1.2",
    "lint-staged": "7.3.0"
  },
  "browserslist": [
    ">0.5%",
    "ie >= 11"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run build",
      "git add"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 80
  },
  "private": true
}
